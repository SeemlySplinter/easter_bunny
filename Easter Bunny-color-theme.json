    "tokenColors": [
      { 
        "name": "fred", 
        "scope": [
          "",
          "punctuation.separator",
          "punctuation.separator.continuation",
          "punctuation.terminator",
          "variable",
          "variable.other.constant",
          "variable.other.enummember",
          "meta.function-call",
          "meta.function-call.arguments.python"
        ],
        "settings":{
          "foreground": "#FFFAF0"
        }
      },
      {
        "scope": [
          "constant.numeric", 
          "punctuation.definition.string.begin",
          "punctuation.definition.string.end"
        ],
        "settings":{
          "foreground": "#EE82EE"
        }
      }, 
      {
        "scope": [
          "string"
        ],
        "settings":{
          "foreground": "#FFFF9F"
        }
      },
      {
        "scope": [
          "constant.language",
          "constant.character.escape"
        ],
        "settings":{
          "foreground": "#9370DB"
        }
      }, 
      {
        "scope": [
          "keyword.operator",
          "keyword.operator.logical.python",
          "storage.modifier",
          "storage.type",
          "keyword.control",
          "support.variable.magic"
        ],
        "settings":{
          "foreground": "#fc86aa"
        }
      },
      {
        "scope": [
          "variable.parameter"
        ],
        "settings":{
          "foreground": "#ADD8E6"
        }
      },
      {
        "scope": [
          "support.function.builtin",
          "entity.name.type",
          "support.type"
        ],
        "settings":{
          "foreground": "#00CCFF"
        }
      },
      {
        "scope": [
          "entity.name.function"
        ],
        "settings":{
          "foreground": "#CCCCFF"
        }
      },
      {
        "scope": [
          "comment"
        ],
        "settings":{
          "foreground": "#778899"
        }
      }
    ]
  }